{
  "productName": "Thinking Space",
  "version": "0.1.0",
  "identifier": "com.thinkingspace.app",
  "build": {
    "beforeDevCommand": "cd src && npm run dev",
    "beforeBuildCommand": "cd src && npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../src/dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "app": {
    "windows": [
      {
        "title": "Thinking Space",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  }
}
